<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<TextView
		android:id="@+id/kategorie_txt"
		android:focusable="false"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingLeft="8dp"
		android:paddingTop="8dp"
		android:text="wand_test"
		android:textSize="12sp"
		android:textColor="#4CAF50"/>
	<LinearLayout
		android:id="@+id/header_with_item"
		android:layout_width="370dp"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/boul_colour123"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:src="@drawable/default_image"
			android:scaleType="center"
			android:scaleY="8.0"/>
		<TextView
			android:id="@+id/element_name123"
			android:focusable="false"
			android:layout_width="80dp"
			android:layout_height="50dp"
			android:padding="8dp"
			android:text="name_test + new\?J/N"
			android:textSize="12sp"
			android:textColor="#000000"/>
		<TextView
			android:id="@+id/wandgenau123"
			android:focusable="false"
			android:layout_width="100dp"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="genaueWand"
			android:textSize="12sp"
			android:textColor="#000000"
			android:layout_weight="1"/>
		<ImageView
			android:id="@+id/flash_image"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/flash_btn"
			android:scaleType="center"
			android:translationX="-10.0dp"
			android:scaleX="0.7"
			android:scaleY="0.7"/>
		<ImageView
			android:id="@+id/top_image"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/top_btn"
			android:scaleType="center"
			android:translationX="-20.0dp"
			android:scaleX="0.7"
			android:scaleY="0.7"/>
		<ImageView
			android:id="@+id/projekt_image"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/projekt"
			android:scaleType="center"
			android:translationX="-30.0dp"
			android:scaleX="0.7"
			android:scaleY="0.7"/>
	</LinearLayout>
</LinearLayout>
